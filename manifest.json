{
	"manifest_version": 2.0,
	"id": "iprep",
	"type": "module",
	"name": "Monitor de BlackList de Domínios",
	"namespace": "IPReputation",
	"version": "1.0",
	"author": "Pedro Santiago",
	"url": "https://github.com/zabbix/community-modules",
	"description": "Módulo para monitorar a reputação de IPs da organização em diversas blacklists e serviços de reputação online.",
	"actions": {
		"module.iprep.list": {
			"class": "IPRepList",
			"view": "iprep.list",
			"layout": "layout.htmlpage"
		},
		"module.iprep.domain": {
			"class": "DomainDashboard",
			"view": "iprep.domain",
			"layout": "layout.htmlpage"
		},
		"iprep.form.edit": {
			"class": "IPRepForm",
			"view": "iprep.form",
			"layout": "layout.json"
		},
		"iprep.form.submit": {
			"class": "IPRepFormSubmit",
			"layout": "layout.json"
		},
		"iprep.form.enable": {
			"class": "IPRepFormStatus",
			"layout": "layout.json"
		},
		"iprep.form.disable": {
			"class": "IPRepFormStatus",
			"layout": "layout.json"
		},
		"iprep.form.delete": {
			"class": "IPRepFormDelete",
			"layout": "layout.json"
		},
		"domain.form.edit": {
			"class": "DomainForm",
			"view": "domain.form",
			"layout": "layout.json"
		},
		"domain.form.submit": {
			"class": "DomainFormSubmit",
			"layout": "layout.json"
		},
		"domain.form.delete": {
			"class": "DomainFormDelete",
			"layout": "layout.json"
		},
		"domain.check": {
			"class": "DomainCheck",
			"layout": "layout.json"
		}
	},
	"storage": {
		"class": "ConfigStorage"
	},
	"assets": {
		"js": ["iprep.js", "iprep.form.js", "domain.js", "domain.form.js"],
		"css": ["iprep.css", "iprep.form.css", "domain.css"]
	}
}
